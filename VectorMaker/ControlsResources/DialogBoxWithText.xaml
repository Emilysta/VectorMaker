<Window x:Class="VectorMaker.ControlsResources.DialogBoxWithText"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VectorMaker.ControlsResources"
        mc:Ignorable="d"    x:Name="DialogBox" d:Width="400" d:Height="150"
        Title="DialogBoxWithText" Height="150" Width="400">
    <Grid>
        <TextBlock Text="0" x:Name="TextBlockObject" Foreground="{StaticResource LightText}"/>
    </Grid>
</Window>
